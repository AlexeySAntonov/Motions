<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="NiceBar">
    <attr name="backgroundTint"/>
    <attr name="leftImageRes"/>
    <attr name="centerImageRes"/>
    <attr name="rightImageRes"/>
    <attr name="leftImageFile"/>
    <attr name="centerImageFile"/>
    <attr name="rightImageFile"/>
    <attr name="fabAlignmentMode">
      <enum name="center" value="0"/>
      <enum name="end" value="1"/>
      <enum name="start" value="2"/>
    </attr>
    <attr name="fabCradleMargin" format="dimension"/>
    <attr name="fabCradleRoundedCornerRadius" format="dimension"/>
    <attr name="fabCradleVerticalOffset" format="dimension"/>
    <attr name="hideOnScroll" format="boolean"/>
    <attr name="barCornersRadius" format="dimension"/>
    <attr name="barTopLeftRadius" format="dimension"/>
    <attr name="barTopRightRadius" format="dimension"/>
    <attr name="barBottomLeftRadius" format="dimension"/>
    <attr name="barBottomRightRadius" format="dimension"/>
    <attr name="barIconsSideMargin" format="dimension"/>
    <attr name="barIconsMode">
      <enum name="imageResources" value="0"/>
      <enum name="lottie" value="1"/>
    </attr>
  </declare-styleable>
</resources>